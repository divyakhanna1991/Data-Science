import pandas as pd

data_frame = pd.read_csv("Covid-19.csv")

random_sample = data_frame.sample(10)
data_frame_filled = data_frame.fillna(0)

numeric_columns = data_frame_filled.select_dtypes(include=[int, float]).columns

population_mean = data_frame_filled[numeric_columns].mean()
sample_mean = random_sample[numeric_columns].mean()

if population_mean.equals(sample_mean):
    comparison_result = "The sample mean is equal to the population mean."
else:
    comparison_result = "The sample mean is not equal to the population mean."

print("\n1. Selecting random sample from the file")
print("\nPrinting Random 10 rows\n")
print(random_sample)

print("\n2. Comparing population mean and sample mean")
print("Population Mean is:")
print(population_mean)
print("\nSample Mean is:")
print(sample_mean)

print("\n3. Displaying result whether sample mean represent the data or not")
print("\nComparison Result:")
print(comparison_result)
